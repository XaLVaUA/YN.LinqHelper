<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<LangVersion>latest</LangVersion>
	<PackageId>YN.LinqHelper.Core</PackageId>
	<Authors>Yaroslav Nozhenko</Authors>
	<RepositoryUrl>https://github.com/XaLVaUA/YN.LinqHelper</RepositoryUrl>
	<Description>Yet another LINQ extension library</Description>
	<PackageTags>linq;helpers;extensions</PackageTags>
	<PackageLicenseExpression>Unlicense</PackageLicenseExpression>
	<SymbolPackageFormat>snupkg</SymbolPackageFormat>
	<Version>1.0.0</Version>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

</Project>
